{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "camelcase" : [
          2,
          {"properties": "always"}
        ]
    },
    "env": {
        "node": true,
        "browser": false
    },
    "extends": "eslint:recommended"
}
